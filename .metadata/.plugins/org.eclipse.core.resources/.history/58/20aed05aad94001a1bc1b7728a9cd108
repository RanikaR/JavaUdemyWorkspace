package section3;

public class Operators {
	
	public static void main(String[] args) {
		
		int result = 1 + 2;
		System.out.println(result);
		
		int prevResult = result;
		result = result - 1;
		
		System.out.println(result);
		
		result = result * 10;
		System.out.println(result);
		
		result = result / 5;
		System.out.println(result);
		
		result = result % 3;
		System.out.println(result);
		
		result++;
		System.out.println(result);
		
		result--;
		System.out.println(result);
		
		
	}

}
