package section4;

public class SatementsWhiteSpaceIndenting {

	public static void main(String[] args) {
		
		// all statements, except for data type and the ;
		int myVariable = 50;
		myVariable++;
		myVariable--;
		
		System.out.println("This is a test"); 
		
		System.out.println("This is " +
							"another " +
							"line.");
		
	}
}
