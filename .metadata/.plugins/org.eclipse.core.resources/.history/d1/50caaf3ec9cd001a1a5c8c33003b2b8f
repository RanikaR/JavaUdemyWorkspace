package section4;

public class MethodOverloadingChallenge {
	
	public static void main(String[] args) {
		
	}
	
	public static int calcFeetInchesToCentimeters(int feet, int inches) {
		if (feet >= 0 ) {
			if (inches >=0 && inches <=12) {
				int justInches = (int) (inches * 2.54);
				System.out.println(inches + " inches is " + justInches + " centimeters.");
				
				
			}
		}
		
		return justInches;
	}
	
	public static int calcFeetInchesToCentimeters(int inches) {
		
	}

}
